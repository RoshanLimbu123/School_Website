<div class="modal fade " id="modalContactForm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
     aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content ">
            <div class="modal-header text-center contact100">
                <h4 class="modal-title w-100 font-weight-bold">Enquiry Form</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <form class="login bg-white shadow p-5" action="/send-enquiry" class="form-group" method="post">
                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true)
                <div class="row">
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label> Name</label>
                            @Html.TextBoxFor(a => a.name, new { @class = "form-control", @required = "required" })
                            <span class="focus-border"></span>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label>Contact Number</label>
                            @Html.TextBoxFor(a => a.contact_number, new { @class = "form-control ", @required = "required", maxlength = "10" })
                            <span class="focus-border"></span>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <label>Address</label>
                    @Html.TextBoxFor(a => a.address, new { @class = "form-control", @required = "required" })
                    <span class="focus-border"></span>
                </div>
                <div class="form-group">
                    <label>Email</label>
                    @Html.TextBoxFor(a => a.email, new { @class = "form-control", @required = "required" })
                    <span class="focus-border"></span>
                </div>
                <div class="form-group">
                    <label>Comment</label>
                    @Html.TextAreaFor(a => a.description, new { @class = "form-control", @required = "required", rows = "4", cols = "50" })
                    <span class="focus-border"></span>
                </div>

                    <button type="submit" style="background-color:blueviolet" class="btn btn-submit">Submit</button>

            </form>
        </div>
    </div>
    <style>
        .contact100 {
            background: #a64bf4;
            background: -webkit-linear-gradient(45deg,#00dbde,#fc00ff);
            background: -o-linear-gradient(45deg,#00dbde,#fc00ff);
            background: -moz-linear-gradient(45deg,#00dbde,#fc00ff);
            background: linear-gradient(45deg,#00dbde,#fc00ff);
        }
    </style>
    <style>

        .center {
            display: block;
            margin-left: auto;
            margin-right: auto;
            width: 50%;
        }
    </style>

    @section Scripts{
        <script>$("#myModal".modal('show');</script>
    }
